{% extends 'tmpl_base.dtl' %}
{% load builderl_dtl_filters %}


{% block title %}{{ project }} at Buildlerl {% endblock %}

{% block body %}
{% for build in builds %}
  <span>{{ build.time | s_to_datetime }}</span> - {{ build.ref }} -
  <a href="/builds/{{ project }}/{{ build.id }}">{{build.commitish}} {{build.id}}</a> - {{ build.state | default:"unknown"}}<br />
{% endfor %}

{% endblock %}
